@using SFO2O.M.Controllers
@using SFO2O.M.ViewModel.Account
@using SFO2O.M.ViewModel.Product
@model IEnumerable<SFO2O.Model.Product.SpuAttributeModel>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.ShowFastShoppingCart = false;
    this.ViewBag.Title = "产品参数";
    SkuAttributeViewModel skuMetaViewModel = ViewBag.SkuMetaViewModel;
    LoginUserModel user = ViewBag.LoginUser ?? null;
}
@section styles {
    <link href="@BundleConfig.BuildCss("Content/css/param.css")" rel="stylesheet">
}
@section scripts {
    <script type="text/javascript" language="javascript" src="@BundleConfig.BuildCss("Scripts/param.js")"></script>
}
@section pageHeader {
    <header class="pageHeader w100 pr tc overflowH">
        <a class="returnBtn" id="returnBtn"></a>
        <b class="f36 FontColor1">产品参数</b>
    </header>
}
<!-- 页头 -->
<!-- 页头 end -->
<table class="w100 f28 paramTable boxShadow">
    @foreach (var m in Model.OrderBy(n => n.SortValue))
    {
        if (!string.IsNullOrEmpty(m.AttributeValue))
        {
            if (m.AttributeName != "是否可换货" && m.AttributeName != "是否可換貨")
            {
            <tr>
                <th>@m.AttributeName</th>
                <td>
                    @m.AttributeValue
                </td>
            </tr>
            }
        }
    }
</table> 