@using SFO2O.M.Controllers
@{
    Layout = "~/Views/Shared/_Layout_login.cshtml";
    ViewData["title"] = ViewBag.Type == 0 ? "健康绿氧注册" : "找回密码";
    ViewBag.Title = ViewData["title"].ToString();
    ViewData["isShowProtocal"] = ViewBag.Type == 0 ? true : false;
    ViewData["mobilePhone"] = ViewBag.mobilePhone;
    ViewData["token"] = ViewBag.token;
    
} 
@section styles{
    <link href="@BundleConfig.BuildCss("Content/css/register.css")" rel="stylesheet">
}
@section scripts{
    <script type="text/javascript">
        var source = '@ViewBag.Type';
    </script>
    @Scripts.Render(BundleConfig.BuildSrc(BundleConfig.BundleJsRegister))
}
@Html.Partial("~/Views/Account/_reg1.cshtml", this.ViewData)
