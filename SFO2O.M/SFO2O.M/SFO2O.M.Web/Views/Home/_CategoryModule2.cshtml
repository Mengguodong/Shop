@{
    Layout = null;
    int index = 0;
}
@using SFO2O.M.Controllers.Common
@using SFO2O.Utility.Uitl
@model List<SFO2O.Model.Index.IndexModulesEntity> 
@if(Model.Count>0){
    foreach(var m in Model){
        string name = "";
        if (m.Sort == 4)
        {
            name = "category2.jpg";
        }
        else
        {
            name = "category3.jpg";
        }
	
        <a href="@m.LinkUrl" target="_self">
            <img src="../../Content/Images/index/@name" alt="@m.Title" class="w100 show lazyloadImg">
		</a>
    }
}
	