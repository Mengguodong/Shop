@model List<SFO2O.Admin.Models.Product.ProductSkuCustomInfoModel>
@if (Model != null && Model.Count > 0)
{
    var index = 0;
    <table class="fff_bor mg_t10" id="cusReports">
        <tr>
            <th width="140" class="vtop">
                <span class="f_l pd_l10 mg_t40">商品SKU報備信息</span>
            </th>
            <td style="background:#fff;">
                @foreach (var pcInfo in Model)
                {
                    <table width="757" class="bg_f1f1f1" style="margin-bottom:15px;">
                        <tr>
                            <th width="88" class="t_r bor_b_e5e5e5">SKU編號：</th>
                            <td width="160" class="bor_b_e5e5e5" id="sku_@index">@pcInfo.Sku</td>
                            <th width="130" class="t_r bor_b_e5e5e5">條形碼：</th>
                            <td width="160" class="bor_b_e5e5e5">@pcInfo.BarCode</td>
                            <th width="82" class="t_r bor_b_e5e5e5">價格：</th>
                            <td width="160" class="bor_b_e5e5e5">$@pcInfo.Price</td>

                        </tr>
                        <tr>
                            <th width="88" class="t_r bor_b_e5e5e5">庫存預警：</th>
                            <td width="160" class="bor_b_e5e5e5">@pcInfo.AlarmStockQty</td>
                            @if (String.IsNullOrWhiteSpace(pcInfo.MainDicValue))
                            {
                                <th width="130" class="t_r bor_b_e5e5e5"></th>
                            }
                            else
                            {
                                <th width="130" class="t_r bor_b_e5e5e5">@pcInfo.MainDicValue：</th>
                            }

                            @if (String.IsNullOrWhiteSpace(pcInfo.MainValue))
                            {
                                <td width="160" class="bor_b_e5e5e5"></td>
                            }
                            else
                            {
                                <td width="160" class="bor_b_e5e5e5">@pcInfo.MainValue</td>
                            }

                            @if (String.IsNullOrWhiteSpace(pcInfo.SubDicValue))
                            {
                                <th width="82" class="t_r bor_b_e5e5e5"></th>
                            }
                            else
                            {
                                <th width="82" class="t_r bor_b_e5e5e5">@pcInfo.SubDicValue：</th>
                            }

                            @if (String.IsNullOrWhiteSpace(pcInfo.SubValue))
                            {
                                <td width="160" class="bor_b_e5e5e5"></td>
                            }
                            else
                            {
                                <td width="160" class="bor_b_e5e5e5">@pcInfo.SubValue</td>
                            }
                        </tr>
                        <tr>
                            <th width="88" class="t_r pd_t10">海關單位：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.CustomsUnit" id="CustomsUnit_@index" /></td>
                            <th width="130" class="t_r pd_t10">產品國檢編號：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.InspectionNo" id="InspectionNo_@index" /></td>
                            <th width="82" class="t_r pd_t10">HS編碼：</th>
                            <td width="160"><input type="text" class="w135 h19 fff_bor mg_t10" value="@pcInfo.HSCode" id="HSCode_@index" /></td>
                        </tr>
                        <tr>
                            <th width="88" class="t_r pd_t10">計量單位：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.UOM" id="UOM_@index" /></td>
                            <th width="130" class="t_r pd_t10">商品備案編號：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.PrepardNo" id="PrepardNo_@index" /></td>
                            <th width="82" class="t_r pd_t10">國條碼：</th>
                            <td width="160"><input type="text" class="w135 h19 fff_bor mg_t10" value="@pcInfo.GnoCode" id="GnoCode_@index" /></td>
                        </tr>
                        <tr>
                            <th width="88" class="t_r pd_t10">稅率：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.TaxRate" id="TaxRate_@index" />%</td>
                            <th width="130" class="t_r pd_t10">行郵稅號：</th>
                            <td width="160"><input type="text" class="w150 h19 fff_bor mg_t10" value="@pcInfo.TaxCode" id="TaxCode_@index" /></td>
                            <th width="82" class="t_r pd_t10"></th>
                            <td width="160"></td>
                        </tr>
                        <tr>
                            <th width="88" class="t_r pd_t10 pd_b10">海關型號：</th>
                            <td width="450" colspan="3"><input type="text" class="h19 fff_bor mg_t10 mg_b10" style="width:444px;" value="@pcInfo.ModelForCustoms" id="ModelForCustoms_@index" /></td>
                            <th width="82" class="t_r"></th>
                            <td width="160"></td>
                        </tr>
                    </table>
                    index++;
                }
            </td>
        </tr>
    </table>
}

